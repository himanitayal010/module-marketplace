        <referenceContainer name="seller.content">
            <block class="Hts\Marketplace\Block\Order\Salesdetail"  name="marketplace_order_salesdetail" template="order/salesdetail.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
